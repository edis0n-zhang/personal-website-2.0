		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.0",
		"@types/node": "^22.10.10",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"gh-pages": "^6.1.1",
		"globals": "^15.0.0",
		"mdsvex": "^0.11.2",
